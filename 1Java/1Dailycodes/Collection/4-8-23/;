import java.util.*;
class ArrayListDemo //extends ArrayList
{
	public static void main(String[] args){

		ArrayList al=new ArrayList();


		al.add(10);
		al.add(20.5f);
		al.add("Shashi");
		al.add(10);
		al.add(20.5f);

		System.out.println(al);

		ArrayList al2=new ArrayList();
		al2.add("SalluBhai");
		al2.add("SarulhBhai");
		al2.add("AmmirKhan");

		al.addAll(al2);
                System.out.println(al);

		al.addAll(3,al2);
		System.out.println(al);
	}
}


